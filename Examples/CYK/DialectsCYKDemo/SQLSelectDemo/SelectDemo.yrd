+start: (create_proc KW_GO)+;
   
create_proc: KW_CREATE (@bad(KW_PROC)|@good(KW_PROCEDURE)) IDENT  AS body;

body: KW_BEGIN (sql_stmt SEMI)+ KW_END ;
fld: expr | expr IDENT | expr AS IDENT; 
sql_stmt: SELECT (fld COMMA)+ fld FROM tbl;
tbl: IDENT;
expr: 
      IDENT
    | LOCALVAR
    | NUM
    | LBR expr RBR
    | expr(PLUS|STAR)expr
    ;
