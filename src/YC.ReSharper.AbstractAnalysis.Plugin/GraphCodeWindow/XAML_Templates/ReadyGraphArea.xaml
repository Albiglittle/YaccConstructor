<!-- EmptyGraphArea with XAML template-->
<graphClasses:EmptyGraphArea 
                                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                                    xmlns:graphClasses="clr-namespace:YC.ReSharper.AbstractAnalysis.Plugin.GraphCodeWindow.GraphClasses"
                                    x:Class="ReadyGraphArea">
    <graphClasses:EmptyGraphArea.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="GraphAreaTemplates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </graphClasses:EmptyGraphArea.Resources>
</graphClasses:EmptyGraphArea>
